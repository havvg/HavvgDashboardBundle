{
    "name": "havvg/dashboard-bundle",
    "description": "A Symfony2 bundle providing a simple unification for dashboards.",
    "keywords": [ "dashboard" ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Toni Uebernickel",
            "email": "tuebernickel@gmail.com"
        }
    ],
    "require": {
        "php": "~5.4",
        "havvg/dry-bundle": "~2.1",
        "twig/twig": "~1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": {
            "Havvg\\Bundle\\DashboardBundle": ""
        }
    },
    "target-dir": "Havvg/Bundle/DashboardBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
